lue="300"/><column index="3" value="63"/><column index="4" value="56"/><column index="5" value="215"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="Venta" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="53"/><column index="2" value="87"/><column index="3" value="84"/><column index="4" value="87"/><column index="5" value="79"/><column index="6" value="71"/><column index="7" value="79"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1"></sql><sql name="Edit View Factura*">CREATE VIEW Factura AS
SELECT
    v.IdVenta AS &quot;NÃºmero Factura&quot;,
    v.Fecha AS &quot;Fecha&quot;,
	c.IdCliente AS &quot;Documento&quot;,
    (COALESCE(c.Nombres, '') || ' ' || COALESCE(c.Apellidos, '')) AS &quot;Cliente&quot;,
    c.Correo,
	ci.Descripcion AS &quot;Ciudad&quot;,
	
    v.Subtotal AS &quot;Subtotal Venta&quot;,
    v.Iva AS &quot;Iva Venta&quot;,
    v.Total AS &quot;Total Venta&quot;



    
FROM Venta v
INNER JOIN Cliente c ON v.IdCliente = c.IdCliente
INNER JOIN Ciudad ci ON c.IdCiudad = ci.IdCiudad


WHERE v.Total &gt; 0
ORDER BY v.IdVenta DESC</sql><current_tab id="1"/></tab_sql></sqlb_project>
