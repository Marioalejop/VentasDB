<?xml version="1.0" encoding="UTF-8"?><sqlb_project><db path="VentasDB.db" readonly="0" foreign_keys="1" case_sensitive_like="0" temp_store="0" wal_autocheckpoint="1000" synchronous="2"/><attached/><window><main_tabs open="structure browser pragmas query" current="1"/></window><tab_structure><column_width id="0" width="300"/><column_width id="1" width="0"/><column_width id="2" width="100"/><column_width id="3" width="5873"/><column_width id="4" width="0"/><expanded_item id="0" parent="1"/><expanded_item id="1" parent="1"/><expanded_item id="2" parent="1"/><expanded_item id="3" parent="1"/></tab_structure><tab_browse><table title="Ciudad" custom_title="0" dock_id="2" table="4,6:mainCiudad"/><dock_state state="000000ff00000000fd0000000100000002000003930000023afc0100000002fb000000160064006f0063006b00420072006f00770073006500310100000000000003930000000000000000fb000000160064006f0063006b00420072006f00770073006500320100000000000003930000011e00ffffff000002690000000000000004000000040000000800000008fc00000000"/><default_encoding codec=""/><browse_table_settings><table schema="main" name="Ciudad" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="58"/><column index="2" value="103"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1"></sql><sql name="Edit View Factura De Venta*">DROP VIEW IF EXISTS &quot;main&quot;.&quot;FacturaDeVenta&quot;;
CREATE VIEW &quot;FacturaDeVenta&quot; AS 
	SELECT V.IdVenta AS &quot;Número de Factura&quot;, V.Fecha AS &quot;Fecha Factura&quot;, 
	C.IdCliente AS &quot;Documento&quot;, concat(C.Nombres, &quot; &quot;, C.Apellidos) AS &quot;Nombre del Cliente&quot;,
	C.Celular AS &quot;Teléfono Contacto&quot;, C.Direccion AS &quot;Dirección Cliente&quot;, 
	Ci.Descripcion AS &quot;Ciudad Cliente&quot;,
	P.IdProducto AS &quot;Código Producto&quot;, P.Descripcion AS &quot;Descripcion Producto&quot;, 
	M.Descripcion AS &quot;Marca&quot;, 
	DP.Serial AS &quot;Serial del Producto&quot;, DP.Color AS &quot;Color del Producto&quot;,
	FP.Descripcion AS &quot;Forma de Pago&quot;,
	DV.Cantidad AS &quot;Cantidad&quot;, DV.ValorUnitario AS &quot;Valor Unitario&quot;, DV.Subtotal, DV.Iva, DV.Total
		
	FROM Producto P, DetalleProducto DP, Marca M, Venta V, Cliente C, Ciudad Ci, DetalleVenta DV, FormaPago FP
	WHERE V.IdVenta = DV.IdVenta AND C.IdCliente = V.IdCliente AND C.IdCiudad = Ci.IdCiudad AND 
	M.IdMarca = P.IdMarca AND P.IdProducto = DP.IdProducto AND DP.IdDetalleProducto = DV.IdDetalleProducto
	AND FP.IdFormaPago = V.IdFormaPago
	ORDER BY V.IdVenta Desc, DV.IdDetalleProducto</sql><current_tab id="1"/></tab_sql></sqlb_project>
